# Sample environment variables for wallpaper-scraper
# Copy this file to .env and replace the placeholder values with your actual API keys
# Then source it before running the scripts: source .env && python main.py

# Unsplash API key (required for Unsplash scraper)
# Get one at: https://unsplash.com/developers
export UNSPLASH_API_KEY="your_unsplash_api_key_here"

# Wallhaven API key (optional, needed for NSFW content)
# Get one at: https://wallhaven.cc/settings/account
export WALLHAVEN_API_KEY="your_wallhaven_api_key_here"

# Pixabay API key (required for Pixabay scraper)
# Get one at: https://pixabay.com/api/docs/
export PIXABAY_API_KEY="your_pixabay_api_key_here"

# Default download directory (optional)
# export WALLPAPER_DOWNLOAD_DIR="~/Pictures/Wallpapers"

# Default rate limit in seconds (optional)
# export WALLPAPER_RATE_LIMIT="1.0"

# Default maximum concurrent downloads (optional)
# export WALLPAPER_MAX_CONCURRENT="3"
